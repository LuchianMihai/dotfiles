<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://openbox.org/
                file:///usr/share/openbox/menu.xsd">

<menu id="root-menu" label="Openbox 3">
  <item label="Terminal emulator">
    <action name="Execute"><execute>st</execute></action>
  </item>
  <separator />
  <item label="Surf Browser">
    <action name="Execute"><execute>surf</execute></action>
  </item>
  <separator />
  <item label="CubeMX">
    <action name="Execute"><execute>cubemx</execute></action>
  </item>
  <item label="Code">
    <action name="Execute"><execute>code</execute></action>
  </item>
  <separator />
  <item label="Openbox Menu">
    <action name="Execute"><execute>st -e vim ~/configs/menu-openbox.xml</execute></action>
  </item>
  <item label="Openbox Config">
    <action name="Execute"><execute>st -e vim ~/configs/rc-openbox.xml</execute></action>
  </item>
  <item label="Reconfigure">
    <action name="Reconfigure" />
  </item>
  <item label="Restart">
    <action name="Restart" />
  </item>
  <separator />
  <item label="Exit">
    <action name="Exit" />
  </item>
</menu>

</openbox_menu>
